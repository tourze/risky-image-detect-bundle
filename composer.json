{
  "name": "tourze/risky-image-detect-bundle",
  "description": "风险图片识别服务",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/event-dispatcher": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "psr/log": "^3|^2|^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\RiskyImageDetectBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\RiskyImageDetectBundle\\Tests\\": "tests"
    }
  }
}